{
  "name": "di-ipv-atp-playbox",
  "description": "GOV.UK 'Base Nunjucks' service",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "engines": {
    "node": "15.14.0"
  },
  "scripts": {
    "precompile": "node generate-files.js",
    "start": "node build/main.js",
    "build": "yarn precompile && webpack --config webpack/webpack.prod.js",
    "buildAndStart": "yarn build && yarn start",
    "build-sass": "rm -rf ./build/style.css && node-sass --include-path scss app/assets/scss/application.scss build/style.css",
    "depcheck": "depcheck",
    "dev": "cross-env NODE_ENV=development ANSI_LOG=true nodemon --trace-warnings app/server.ts",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "yarn run lint --fix",
    "lint:staged": "lint-staged",
    "test": "yarn precompile && yarn test:unit",
    "test:unit": "rm -rf test/coverage && cross-env NODE_ENV=test nyc mocha --exit --recursive \"test/unit/**/*.test.ts\" --recursive \"app/**/*.test.ts\"",
    "test:coverage": "nyc report",
    "codecept:e2e": "codeceptjs run -c codecept.conf.js --verbose",
    "codecept:e2e:parallel": "codeceptjs run-workers 6 -c codecept.conf.js --verbose",
    "codecept:e2e:ci": "codeceptjs run-workers 4 -c codecept.conf.js",
    "e2e": "cross-env NODE_ENV=development ENVIRONMENT=prod yarn codecept:e2e -g @e2e",
    "e2e:parallel": "cross-env NODE_ENV=development ENVIRONMENT=prod yarn codecept:e2e:parallel -g @e2e",
    "e2e:flow": "cross-env NODE_ENV=development ENVIRONMENT=prod yarn codecept:e2e -g $FLOW",
    "e2e:wip": "cross-env NODE_ENV=development ENVIRONMENT=prod yarn codecept:e2e -g @wip",
    "e2e:ci": "cross-env NODE_ENV=development cross-env ENVIRONMENT=prod cross-env LOGS_LEVEL=error yarn codecept:e2e:ci -g @e2e",
    "test:all": "yarn precompile && yarn lint && yarn test:coverage && yarn test:unit && yarn e2e "
  },
  "nyc": {
    "exclude": [
      "app/features/**/__tests__/",
      "app/features/**/index.ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "yarn run lint:fix"
    ]
  },
  "resolutions": {
    "https-proxy-agent": "^5.0.0",
    "@types/express-session": "~1.15.15"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.4.1",
    "axios": "^0.21.1",
    "babel-loader": "^8.0.6",
    "cfenv": "^1.2.4",
    "connect-redis": "^4.0.3",
    "cookie-parser": "^1.4.4",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "csurf": "^1.11.0",
    "dompurify": "^2.0.7",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-useragent": "^1.0.15",
    "express-validator": "^6.6.0",
    "govuk-frontend": "^3.7.0",
    "helmet": "^3.21.2",
    "http-status-codes": "^1.3.2",
    "i18next": "^19.5.2",
    "i18next-fs-backend": "^1.0.7",
    "i18next-http-middleware": "^3.0.1",
    "jsdom": "^15.2.0",
    "jsdom-global": "^3.0.2",
    "jsonwebtoken": "^8.5.1",
    "mini-css-extract-plugin": "^0.8.0",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.33",
    "moment-timezone-data-webpack-plugin": "^1.4.0",
    "nocache": "^2.1.0",
    "node-jose": "^2.0.0",
    "nunjucks": "^3.2.0",
    "openid-client": "^4.2.1",
    "pem-jwk": "^2.0.0",
    "prettier": "^2.2.1",
    "punycode": "^2.1.1",
    "redis": "^3.0.2",
    "sass-loader": "^10.2.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^6.2.1",
    "uuid": "^8.3.2",
    "webpack": "^4.41.2",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@codeceptjs/configure": "^0.5.2",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.3",
    "@types/chai-as-promised": "^7.1.3",
    "@types/connect-redis": "^0.0.11",
    "@types/express": "^4.17.1",
    "@types/express-session": "~1.15.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.5",
    "@types/nunjucks": "^3.1.2",
    "@types/redis": "^2.8.23",
    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.3",
    "@types/webpack-dev-middleware": "^2.0.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.5.0",
    "codeceptjs": "^2.6.6",
    "cross-env": "^5.2.1",
    "depcheck": "^1.4.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.2.11",
    "mocha": "^6.2.0",
    "node-sass": "^5.0.0",
    "nodemon": "^1.19.2",
    "nyc": "^14.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pa11y": "^5.3.0",
    "pa11y-reporter-html": "^2.0.0",
    "pseudo-localization": "^2.1.1",
    "sass": "^1.26.9",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "source-map-support": "^0.5.13",
    "terser-webpack-plugin": "^2.1.2",
    "ts-node": "^8.4.1",
    "typescript": "^4.0.2",
    "uuid-validate": "^0.0.3",
    "vue": "3.0.11",
